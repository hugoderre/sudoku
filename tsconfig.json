{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "downlevelIteration": true,
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "declaration": true,
        "declarationDir": "./dist/typings",
        "strict": true,
        "strictNullChecks": true,
        "checkJs": true,
    }
}